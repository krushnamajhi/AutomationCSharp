{
  "Environment": "UAT",
  "Number of Browsers": 2,
  "RunTestScriptValidator": false,
  "DefaultBrowser": {
    "UseDefaultBrowser": true,
    "BrowserName": "Chrome"
  },
  "DriverSettings": {
    "DriverFolder": {
      "DefaultPath": true,
      "Path": "/../CommonRepos/Drivers"
    },
    "DriverConfigs": {
      "Folder": {
        "DefaultPath": true,
        "Path": "/../CommonRepos/Drivers"
      },
      "ConfigFiles": {
        "Chrome": "ChromeConfig.txt",
        "Edge": "EdgeConfig.txt",
        "Firefox": "FirefoxConfig.txt"
      }
    }
  },
  "TestSettings": {
    "ResourceFolder": {
      "DefaultPath": true,
      "Path": "\\Resources"
    },
    "TestDataFolder": {
      "DefaultPath": true,
      "Path": "\\Resources\\TestData"
    },
    "TestScriptFolder": {
      "DefaultPath": true,
      "Path": "\\Resources\\TestScript"
    },
    "ObjectRepositoryFolder": {
      "DefaultPath": true,
      "Path": "\\Resources\\ObjectRepository"
    },
    "TestSuiteFolder": {
      "DefaultPath": true,
      "Path": "\\Resources\\TestSuite"
    },
    "SingleSheet": {
      "DefaultPath": true,
      "Path": "\\Resources\\SingleSheet.xlsx"
    }
  },
  "ReportSettings": {
    "ResultsFolder": {
      "DefaultPath": true,
      "Path": "/../TestResults"
    },
    "ScreenshotFolderName": "Screenshots"
  }
}
